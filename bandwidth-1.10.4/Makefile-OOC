# Note: 32-bit is no longer supported by Mac OS.

OOCLIB32=OOC/OOC32.a
OOCLIB64=OOC/OOC64.a

OOC32:
	( cd OOC; make OOC32.a )

OOC64:
	( cd OOC; make OOC64.a )

ARMOOC32:
	( cd OOC; make ARMOOC32 )

ARMOOC64:
	( cd OOC; make ARMOOC64 )

